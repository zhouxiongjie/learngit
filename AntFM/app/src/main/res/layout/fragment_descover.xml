<com.qmuiteam.qmui.widget.QMUIWindowInsetLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipChildren="false"
    app:qmui_skin_background="?attr/app_skin_common_background">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <View
            android:id="@+id/statusBar"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="?attr/themeColor" />

        <com.shuangling.software.customview.TopTitleBar
            android:id="@+id/activtyTitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/statusBar"
            android:background="?attr/themeColor"
            android:visibility="visible"
            app:canBack="false"
            app:moreImg="@drawable/ic_more" />

        <!--<ProgressBar
            android:id="@+id/progressBar"
            style="@style/WebViewProgressBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/activtyTitle"
            android:progressDrawable="@drawable/webview_progress_drawable"
            android:max="100"
            android:progress="10"
            android:visibility="gone" />

        <android.webkit.WebView
            android:id="@+id/webView"
            android:layout_below="@+id/progressBar"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />-->
        <include
            layout="@layout/activity_webview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/activtyTitle" />
    </RelativeLayout>
</com.qmuiteam.qmui.widget.QMUIWindowInsetLayout>

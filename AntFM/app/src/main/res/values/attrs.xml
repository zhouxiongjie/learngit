<?xml version="1.0" encoding="utf-8"?>
<resources>


    <attr name="themeColor" format="reference"/>

    <declare-styleable name="TopTitleBar">
        <!-- 标题文案 -->
        <attr name="titleText" format="string"/>
        <!-- 是否有back按钮 -->
        <attr name="canBack" format="boolean"/>
        <!-- back按钮文案 -->
        <attr name="backText" format="string"/>
        <!-- 右侧更多功能按钮图片 -->
        <attr name="moreImg" format="reference"/>
        <!-- 右侧更多功能按钮文字 -->
        <attr name="moreText" format="string"/>
    </declare-styleable>


    <declare-styleable name="ProgressCircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <declare-styleable name="LineWrapLayout">
        <attr name="magin" format="integer" />
        <attr name="itemBg" format="reference"/>
    </declare-styleable>

</resources>
